@{
    ViewBag.Title = "Index";
    Layout = null;
}


<!DOCTYPE html>
<html ng-app="quoteApp">
<head>
    @*<base href="/">*@
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
    <title ng-bind="title + ' | Moovers'">Cube - Bootstrap Admin Template</title>
    @if (ViewBag.IsDevelopment)
    {
        foreach (string file in ViewBag.JavascriptFiles)
        {
            @Html.Javascript(file)
        }
        foreach (string file in ViewBag.ScreenCssFiles)
        {
            @Html.Stylesheet(file, "screen")
        }
        foreach (string file in ViewBag.PrintCssFiles)
        {
            @Html.Stylesheet(file, "print")
        }
    }
    else
    {
        @Html.Stylesheet("~/static/compiled/print.css", "print")
        @Html.Stylesheet("~/static/compiled/site.css", "screen")
        @Html.Javascript("~/static/compiled/js.js")
    }

    <link href='http://fonts.googleapis.com/css?family=Open+Sans:400,600,700,300|Titillium+Web:200,300,400' rel='stylesheet' type='text/css'>
    <script type="text/javascript" src="https://maps.googleapis.com/maps/api/js?key=AIzaSyASm3CwaK9qtcZEWYa-iQwHaGi3gcosAJc&sensor=false"></script>
    <!--[if lt IE 9]>
        <script src="js/html5shiv.js"></script>
        <script src="js/respond.min.js"></script>
    <![endif]-->


    <script type="text/javascript">

        function OpenDialog(id) {
            $('#' + id).modal('show');


        };

        function OpenDialogT(id) {

            $('#' + id).removeClass('md-hide');
            $('#' + id).addClass('md-show');
            $('.md-overlay').css('visibility', 'visible');
            $('.md-overlay').css('opacity', '1');

        };

        function CloseDialogT(id) {
            
            
            $('#' + id).addClass('md-hide');
            $('.md-overlay').css('visibility', 'hidden');
            $('.md-overlay').css('opacity', '0');
            $('#' + id).removeClass('md-show');


        };


    </script>
</head>
<body class="pace-done theme-red">
