@model dynamic
<!DOCTYPE html>
<html>
<head>
    <meta content="text/html; charset=utf-8" http-equif="Content-Type">
</head>
<body>
    <table width="100%" cellpadding="0" cellspacing="0" align="center">
        <tbody>
            <tr>
                <td align="center" width="600">
                    <div align="center" style="max-width: 600px; min-width: 100px; text-align: left;">
                        <table width="600" align="center" cellspacing="0" border="0" cellpadding="0" class="preheader" style="font-family: Arial, Helvetica, sans-serif; font-weight: normal; font-size: 12px; line-height: 19px; color: #444444; border-collapse: collapse;">
                            <tbody>
                                <tr>
                                    <td>
                                        <div data-browser="useragent" style="text-align: right;">
                                            @if (ViewBag.ShowEmailLink == null && ViewBag.ViewLink != null)
                                            {
                                                <span style="font-size: 11px; color: #808080; margin-bottom: 0;">Email not displaying correctly?
                                                </span>
                                                <a href="@ViewBag.ViewLink">View it in your browser</a>
                                            }
                                        </div>
                                    </td>
                                </tr>
                            </tbody>
                        </table>
                        <table id="body" style="text-align: left; width: 600px; max-width: 600px; min-width: 200px; border: solid 1px #e9e9e9; font-family: Arial, Helvetica, sans-serif; color: #333333; margin: 0 auto;" cellpadding="0" cellspacing="0">
                            <tbody>
                                <tr>
                                    <td style="background-color: #f5f5f5; padding: 15px;">
                                        <img src="http://confirm.1800moovers.com/static/img/headerLogoSmall.png" title="Moovers Moving and Storage" alt="Moovers Moving and Storage" />
                                    </td>
                                    <td style="background-color: #f5f5f5; color: #444444; font-size: 12px; padding: 15px; text-align: right;">
                                        @RenderSection("HeaderMessage", false)
                                    </td>
                                </tr>
                                <tr>
                                    <td colspan="2" style="padding: 15px;">
                                        @RenderBody()
                                    </td>
                                </tr>
                            </tbody>
                        </table>
                        <div style="text-align: center !important; line-height: 1.2em; font-size: 10px; font-family: Arial, Helvetica, sans-serif; color: #666666; padding: 12px 20px;">
                            Each franchise independently owned and operated.
                        </div>
                        <div style="text-align: center; margin-bottom: 20px;">
                            <img src="http://confirm.1800moovers.com/static/img/footerLogo.png" alt="Moovers Moving and Storage" />
                        </div>
                        <div style="text-align: center; font-size: 10px; font-family: Helvetica, Arial, 'DejaVu Sans', 'Liberation Sans', Freesans, sans-serif; color: #666">
                            @RenderSection("UnsubscribeContent", false)
                        </div>
                    </div>
                </td>
            </tr>
        </tbody>
    </table>
</body>
</html>